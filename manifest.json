{
  "manifest_version": 3,
  "name": "scroll zoom for mac",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "src/worker.js"
  },
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/content.js"]
  }]
}
